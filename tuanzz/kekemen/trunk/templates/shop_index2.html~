	function showCate() {
		var key = document.getElementById("key");
		var search_cate = document.getElementById("search_cate");
		key.onblur = function() {
			search_cate.style.display="none"; 
		}
		key.onfocus = function() {
			search_cate.style.display="block";
		}
	}
	
	window.onload = function() {
		showCate();
	}

</script>
